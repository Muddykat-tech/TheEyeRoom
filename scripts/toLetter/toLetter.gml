function toLetter(num){
	switch(num){
		case 0:
			return "A";
		break;
		case 1:
			return "B";
		break;
		case 2:
			return "C";
		break;
		case 3:
			return "D";
		break;
		case 4:
			return "E";
		break;
	}
}